<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="EmptyWorker" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <VirtualMachineRole  name="EmptyWorkerRole" vmsize="Small">
    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
      <Import moduleName="Diagnostics" />
    </Imports>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="tcp" port="80" />
    </Endpoints>
  </VirtualMachineRole>
</ServiceDefinition>